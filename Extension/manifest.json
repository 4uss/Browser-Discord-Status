{
   "manifest_version": 2,
   "name": "Browser Discord Status",
   "version": "1.0.4",
   "description": "Show to your friends what you are doing in browser.",
   "permissions": [ "tabs", "notifications" ],

   "background": {
      "page": "bg.html"
   },
   "browser_action": {
      "default_icon": {
         "16": "icons/16.png",
         "32": "icons/32.png"
      },
      "default_popup": "popup.html"
   },
   "icons": {
      "128": "icons/128.png",
      "48": "icons/48.png"
   }
}